doctype html
html
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    meta(name="description" content="A starter repo for Gulp, Jade, Sass, jQuery, and Bootstrap")
    title Gulp | Jade | Sass | jQuery | Bootstrap
    link(rel='stylesheet', href='lib/build.css')
    link(rel='stylesheet', href='main.css')
    //- link(rel="icon", type="image/png", href="assets/favicon32.png", sizes="32x32")
    //- link(rel="icon", type="image/png", href="assets/favicon16.png", sizes="16x16")
  body
    .board
      .pads
        #bass.pad
        #snare.pad
        #closedHat.pad
        #clap.pad
      .sequencer
        - for (var i = 0; i < 2; i++)
          .note.theone
          .note
          .note
          .note
          .note
          .note
          .note
          .note
      .buttons
        .play PLAY
        .pause
        .stop STOP
    script(src='lib/build.js')
    script(src='lib/lowLag.js')
    script(src='lib/randomColor.js')
    script(src='main.js')
